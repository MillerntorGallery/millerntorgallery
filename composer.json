        "typo3/cms": "7.6.6",
        "typo3-ter/theme-bootstrap": "7.0.0",
        "plusb/pb_social": "1.3.1",
        "vca/theme-millerntor": "dev-master as 7.0.0",
    },
    "license": "GPL",
    "authors": [
        {
            "name": "Sebastian Fuchs",
            "email": "sfuchs@projektkater.de"
        }
