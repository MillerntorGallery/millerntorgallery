{   "repositories": [
        { "type": "composer", "url": "https://composer.typo3.org/" },
        { "type": "vcs", "url": "https://github.com/MillerntorGallery/theme_millerntor"},
        { "type": "vcs", "url": "https://github.com/typo3-themes/themes"},
        { "type": "vcs", "url": "https://github.com/TYPO3-extensions/gridelements"},
        { "type": "vcs", "url": "https://bitbucket.org/plus-b/pb_social.git"}
    ],
    "name": "vca/millerntorgallery",
