{   "repositories": [
        { "type": "composer", "url": "https://composer.typo3.org/" },
        { "type": "vcs", "url": "https://github.com/MillerntorGallery/theme_millerntor"},
        { "type": "vcs", "url": "https://github.com/typo3-themes/themes"},
        { "type": "vcs", "url": "https://github.com/typo3-themes/theme_bootstrap"},
        { "type": "vcs", "url": "https://github.com/TYPO3-extensions/gridelements"},
        { "type": "vcs", "url": "https://bitbucket.org/plus-b/pb_social.git"},
        { "type": "vcs", "url": "https://github.com/b13/t3ext-seo_basics"},
        { "type": "vcs", "url": "https://github.com/MillerntorGallery/vca_millerntor"},
        { "type": "vcs", "url": "https://github.com/jonathanheilmann/ext-jh_opengraphprotocol"}
    ],
    "name": "vca/millerntorgallery",
    "description": "Site-Package for millerntorgallery.org",
    "type": "project",
    "require": {
        "typo3/cms": "^7.6",
        "plusb/pb_social": "1.3.1",
        "vca/theme-millerntor": "dev-master as 7.0.1",
        "typo3-ter/realurl": "^2.3",
        "b13/seo_basics": "^0.9",
        "typo3-ter/cal": "^1.11",
        "jonathanheilmann/jh-opengraphprotocol": "^1.3",
        "rlmp/rlmp-language-detection": "^7.0",
        "reelworx/rx-shariff": "^10.3",
        "typo3-ter/lfeditor": "^4.2",
        "vca/vca-millerntor": "dev-master as 7.0.0",
        "typo3-ter/mediace": "^7.6"
    },
    "support": {
        "issues": "https://github.com/MillerntorGallery/millerntorgallery/issues",
        "source": "https://github.com/MillerntorGallery/millerntorgallery",
        "docs": "https://github.com/MillerntorGallery/millerntorgallery/blob/master/README.md"
    },
    "license": "GPL",
    "authors": [
        {
            "name": "Sebastian Fuchs",
            "email": "sfuchs@projektkater.de"
        }
    ],
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ""
        }
    },
    "autoload": {
        "psr-4": {
          "VCA\\VcaMillerntor\\": "typo3conf/ext/vca_millerntor/Classes/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6.32"
        }
    }
}
